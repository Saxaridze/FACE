<Page x:Class="FACE.Pages.PageMainTableBtn"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FACE.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Таблицы">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0"/>
            <RowDefinition/>
            <RowDefinition Height="0"/>
        </Grid.RowDefinitions>
        <UniformGrid Grid.Row="1" >
            <Button x:Name="BtnService" Click="BtnService_Click" Style="{StaticResource MenuButton}">Service</Button>
            <Button x:Name="BtnClient" Click="BtnClient_Click" Style="{StaticResource MenuButton}">Client</Button>
            <Button x:Name="BtnClientService" Click="BtnClientService_Click" Style="{StaticResource MenuButton}">ClientService</Button>
            <Button x:Name="BtnGender" Click="BtnGender_Click" Style="{StaticResource MenuButton}">Gender</Button>
        </UniformGrid>
    </Grid>
</Page>
